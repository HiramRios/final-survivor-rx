    // Function to add a product to the cart via AJAX
    function addToCartNew(variantId, quantity, selling_plan_id) {
      // Prepare the data to be sent to the Shopify cart
      var buttonText = document.getElementsByClassName(variantId)[0].getElementsByClassName("buttonText")[0]
      var spinner = document.getElementsByClassName(variantId)[0].getElementsByClassName("spinner")[0];
    
      buttonText.style.display = "none";
      spinner.style.display = "inline-block";
      var data = {
        items: [{
          id: variantId,  // Product variant ID (not product ID)
          quantity: quantity,  // Quantity to add
          selling_plan: selling_plan_id,
        }]
      };
    
      // Make the AJAX request to add the product to the cart
      fetch('/cart/add.js', {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json',
        },
        body: JSON.stringify(data)
      })
      .then(response => response.json())
      .then(cart => {
        setTimeout(function() {
            spinner.style.display = "none";
            buttonText.style.display = "inline-block";
        }, 100);
      })
      .catch(error => {
        console.error('Error adding to cart:', error);
        setTimeout(function() {
            spinner.style.display = "none";
            buttonText.style.display = "inline-block";
        }, 100);
      });
    }
