        async function fetchMetaobjectById(metaobjectId, metaFieldName) {
          const query = `
            query getMetaobject($id: ID!) {
              metaobject(id: $id) {
                id
                type
                fields {
                  key
                  value
                }
              }
            }
          `;
          const imageQuery = `
            query getMediaImage($id: ID!) {
              node(id: $id) {
                ... on MediaImage {
                  id
                  image {
                    url
                    altText
                    width
                    height
                  }
                }
              }
            }
          `;
              
          const response = await fetch(`https://${Shopify.shop}//api/2023-10/graphql.json`, {
            method: 'POST',
            headers: {
              'Content-Type': 'application/json',
              'X-Shopify-Storefront-Access-Token': '526ac0561d3c634d10ae7135db214679',
            },
            body: JSON.stringify({
              query,
              variables: {
                id: metaobjectId,
              },
            }),
          });

          const result = await response.json();

          const id = result?.data?.metaobject?.fields?.find(f => f.key === "image")?.value;

          const image_response = await fetch(`https://${Shopify.shop}//api/2023-10/graphql.json`, {
            method: 'POST',
            headers: {
              'Content-Type': 'application/json',
              'X-Shopify-Storefront-Access-Token': '526ac0561d3c634d10ae7135db214679',
            },
            body: JSON.stringify({
              query: imageQuery,
              variables: {
                id: id,
              },
            }),
          });

          const image_result = await image_response.json();
    
          const metaobjectInfo = () => {
            // Create an object containing the heading, subheading, and quiz_short_description
            return {
              heading: result?.data?.metaobject?.fields?.find(f => f.key === "heading")?.value || "No heading available",
              sub_heading: result?.data?.metaobject?.fields?.find(f => f.key === "sub_heading")?.value || "No subheading available",
              quiz_short_description: result?.data?.metaobject?.fields?.find(f => f.key === "quiz_short_description")?.value || "No quiz_short_description listed",
              image: image_result.data.node.image.url || "",
            };
          };
    
          const categoriesMetaobjectInfo = () => {
            // Create an object containing the heading, subheading, and quiz_short_description
            return {
              title: result?.data?.metaobject?.fields?.find(f => f.key === "quiz_add_on_support_categories")?.value || "No heading available",
              description: result?.data?.metaobject?.fields?.find(f => f.key === "quiz_add_on_support_type_description")?.value || "No subheading available",
            };
          };
    
          if(['survivor_essential_support', 'cancer_specific_support', 'immune_support', 'inside_the_package'].includes(metaFieldName)) {

            return metaobjectInfo();
          } else if ('quiz_add_on_categories' === metaFieldName) {
            return categoriesMetaobjectInfo();
          }
        }
    
