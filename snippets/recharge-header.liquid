  @media (max-width: 768px) {
    .yno-label {
      display: none;
    }
  }
  .yno-cancel span {
    font-weight: 500;
  }
  .cancel-popup {
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    opacity: 0;
    z-index: -1;
    display: flex;
    align-items: center;
    justify-content: center;
    isolation: isolate;
    transition-property: opacity;
    transition-timing-function: ease;
    transition-duration: var(--_1mi3r8xm);
    will-change: opacity;
    color: var(--recharge-typography-primary);
