    color: #2d9e4f;
    font-size: var(--recharge-typography-size-5);
    font-weight: 600;
    line-height: 150%;
    background: transparent;
    border-radius: 24px;
    margin-top: 12px;
  }
  .cancel-popup__cancel:hover {
    color: #fff;
  }
  @media (max-width: 768px) {
    .cancel-popup__inner {
      border-bottom-left-radius: 0;
      border-bottom-right-radius: 0;
    }
