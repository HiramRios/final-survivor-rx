  }
  .cancel-popup__close__btn {
    width: 32px;
    height: 32px;
    padding: 4px;
  }
  .cancel-popup__heading {
    padding-right: 32px;
  }
  .cancel-popup__heading h3 {
    font-size: var(--recharge-typography-size-3);
    line-height: 123%;
    font-weight: bold;
  }
  .cancel-popup__heading p {
    font-size: var(--recharge-typography-size-5);
    line-height: 150%;
  }
  .cancel-popup__option {
    position: relative;
    margin-top: 16px;
  }
  .cancel-popup__option label {
    padding: 12px 12px 12px 42px;
    border-radius: 8px;
    background-color: #dfe5e1;
    text-align: left;
    transition-property: background-color;
    transition-timing-function: ease;
    transition-duration: .3s;
    cursor: pointer;
    font-size: var(--recharge-typography-size-5);
