    font-size: 16px;
    display: flex;
    align-items: center;
    gap: 7px;
    text-align: right;
    justify-content: flex-end;
    /* margin: 30px auto; */
    padding: 0;
    width: 100%;
    margin-top: 5px;
  }
  .yno-label {
    display: block;
    font-size: var(--recharge-typography-size-5);
    line-height: 150%;
    font-weight: var(--recharge-typography-body-font-weight);
