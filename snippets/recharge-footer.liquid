      async cancelSubscriptions() {
          try {
              const nextQueuedCharges = await this.nextQueuedCharges();
              
              if (!nextQueuedCharges?.length) {
                  console.error('No queued charges found');
                  return;
              }

              let reason = $('[name="reason-popup__month"]:checked').val();
              let reasonNote = '';
